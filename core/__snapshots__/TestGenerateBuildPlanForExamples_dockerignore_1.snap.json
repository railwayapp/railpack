{
 "deploy": {
  "base": {
   "image": "ghcr.io/railwayapp/railpack-runtime:latest"
  },
  "inputs": [
   {
    "include": [
     "."
    ],
    "step": "build"
   }
  ],
  "startCommand": "sh start.sh"
 },
 "exclude": [
  ".vscode",
  ".copier",
  ".env-specific",
  ".env*",
  "__pycache__",
  "test",
  "tmp/*",
  "*.log",
  "Justfile",
  "TODO*",
  "README.md",
  "docker-compose*.yml",
  "negation_test/*"
 ],
 "include": [
  "negation_test/should_exist.txt",
  "negation_test/existing_folder"
 ],
 "steps": [
  {
   "inputs": [
    {
     "image": "ghcr.io/railwayapp/railpack-builder:latest"
    }
   ],
   "name": "packages:mise"
  },
  {
   "commands": [
    {
     "cmd": "chmod +x start.sh"
    }
   ],
   "inputs": [
    {
     "step": "packages:mise"
    },
    {
     "include": [
      "."
     ],
     "local": true
    }
   ],
   "name": "build",
   "secrets": [
    "*"
   ]
  }
 ]
}