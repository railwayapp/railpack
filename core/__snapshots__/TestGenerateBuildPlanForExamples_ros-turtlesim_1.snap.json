{
 "deploy": {
  "base": {
   "image": "osrf/ros:kilted-desktop"
  },
  "inputs": [
   {
    "step": "apt"
   },
   {
    "include": [
     "."
    ],
    "step": "build"
   }
  ],
  "startCommand": "source /opt/ros/kilted/setup.bash \u0026\u0026 source install/setup.bash \u0026\u0026 ros2 launch launch/multisim.launch.py"
 },
 "steps": [
  {
   "inputs": [
    {
     "image": "osrf/ros:kilted-desktop"
    }
   ],
   "name": "image"
  },
  {
   "commands": [
    {
     "cmd": "apt-get update"
    },
    {
     "cmd": "apt-get install -y python3-colcon-common-extensions python3-rosdep ros-dev-tools ros-kilted-desktop"
    },
    {
     "cmd": "apt-get install -y ros-kilted-ament-cmake ros-kilted-ament-cmake-core ros-kilted-ament-cmake-python"
    }
   ],
   "inputs": [
    {
     "step": "image"
    }
   ],
   "name": "apt",
   "secrets": [
    "*"
   ]
  },
  {
   "commands": [
    {
     "cmd": "rosdep update"
    },
    {
     "cmd": "rosdep install --from-paths src --ignore-src -r -y"
    },
    {
     "cmd": "bash -c 'source /opt/ros/kilted/setup.bash \u0026\u0026 colcon build'"
    }
   ],
   "inputs": [
    {
     "step": "apt"
    },
    {
     "include": [
      "."
     ],
     "local": true
    }
   ],
   "name": "build",
   "secrets": [
    "*"
   ]
  }
 ]
}