{
 "deploy": {
  "base": {
   "image": "ghcr.io/railwayapp/railpack-runtime:latest"
  },
  "inputs": [
   {
    "include": [
     "."
    ],
    "step": "defaultsToUsing"
   },
   {
    "include": [
     "."
    ],
    "step": "doesNotUseSecrets"
   },
   {
    "include": [
     "."
    ],
    "step": "usesSecrets"
   }
  ],
  "startCommand": "./run.sh"
 },
 "secrets": [
  "MY_SECRET",
  "MY_OTHER_SECRET",
  "HELLO_WORLD"
 ],
 "steps": [
  {
   "assets": {
    "mise.toml": "[mise.toml]"
   },
   "commands": [
    {
     "path": "/mise/shims"
    }
   ],
   "inputs": [
    {
     "image": "ghcr.io/railwayapp/railpack-builder:latest"
    }
   ],
   "name": "packages:mise",
   "variables": {
    "MISE_CACHE_DIR": "/mise/cache",
    "MISE_CONFIG_DIR": "/mise",
    "MISE_DATA_DIR": "/mise",
    "MISE_INSTALLS_DIR": "/mise/installs",
    "MISE_SHIMS_DIR": "/mise/shims"
   }
  },
  {
   "commands": [
    {
     "dest": ".",
     "src": "."
    },
    {
     "cmd": "sh -c './run.sh'",
     "customName": "./run.sh"
    }
   ],
   "inputs": [
    {
     "step": "packages:mise"
    }
   ],
   "name": "defaultsToUsing",
   "secrets": [
    "*"
   ],
   "variables": {
    "NOT_SECRET": "not secret"
   }
  },
  {
   "commands": [
    {
     "dest": ".",
     "src": "."
    },
    {
     "cmd": "sh -c './run.sh true'",
     "customName": "./run.sh true"
    }
   ],
   "inputs": [
    {
     "step": "packages:mise"
    }
   ],
   "name": "doesNotUseSecrets"
  },
  {
   "commands": [
    {
     "dest": ".",
     "src": "."
    },
    {
     "cmd": "sh -c './run.sh'",
     "customName": "./run.sh"
    }
   ],
   "inputs": [
    {
     "step": "packages:mise"
    }
   ],
   "name": "usesSecrets",
   "secrets": [
    "MY_SECRET",
    "MY_OTHER_SECRET"
   ],
   "variables": {
    "NOT_SECRET": "not secret"
   }
  }
 ]
}